{
  "name": "di-express",
  "version": "0.0.1",
  "description": "Implement dependency injection in epress without using orm",
  "main": "main.ts",
  "scripts": {
    "write:env": "ts-node ./scripts/writeEnv.ts",
    "migration:init": "ts-node-dev -r tsconfig-paths/register ./scripts/migration.ts --init ",
    "migration:drop": "ts-node-dev -r tsconfig-paths/register ./scripts/migration.ts --drop",
    "migration:mock": "ts-node-dev -r tsconfig-paths/register ./scripts/migration.ts --mock",
    "start:dev": "pnpm write:env && ts-node-dev --transpile-only -r tsconfig-paths/register ./src/main.ts"
  },
  "keywords": ["Express", "dependency Injection"],
  "author": "public.winverse@gmail.com",
  "license": "MIT",
  "dependencies": {
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "cookie-parser": "^1.4.6",
    "cuid": "^2.1.8",
    "date-fns": "^2.29.3",
    "dotenv": "^16.0.3",
    "express": "^4.18.2",
    "joi": "^17.7.0",
    "jsonwebtoken": "^8.5.1",
    "node-mocks-http": "^1.11.0",
    "pg": "^8.8.0",
    "reflect-metadata": "^0.1.13",
    "tsyringe": "^4.7.0",
    "winston": "^3.8.2",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "@types/cookie-parser": "^1.4.3",
    "@types/express": "^4.17.14",
    "@types/jest": "^29.2.2",
    "@types/jsonwebtoken": "^8.5.9",
    "@types/node": "^18.11.9",
    "@types/pg": "^8.6.5",
    "@typescript-eslint/eslint-plugin": "^5.42.1",
    "@typescript-eslint/parser": "^5.42.1",
    "eslint": "^8.27.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-prettier": "^4.2.1",
    "jest": "^29.3.0",
    "prettier": "^2.7.1",
    "ts-jest": "^29.0.3",
    "ts-node": "^10.9.1",
    "ts-node-dev": "^2.0.0",
    "tsconfig-paths": "^4.1.0",
    "typescript": "^4.8.4"
  }
}
